{
  "apiType": "stageMode",
  "buildOption": {
    "strictMode": {
      "caseSensitiveCheck": true
    }
  },
  "modules": [
    {
      "name": "entry",
      "srcPath": "./entry",
      "targets": [
        {
          "name": "default",
          "applyToProducts": ["default"]
        }
      ]
    }
  ],
  "products": [
    {
      "name": "default",
      "signingConfig": "default",
      "compatibleSdkVersion": "4.0.0(10)",
      "runtimeOS": "HarmonyOS"
    }
  ],
  "signingConfigs": [
    {
      "name": "default",
      "type": "HarmonyOS",
      "material": {
        "certpath": "build/default/intermediates/signing/debug/cert.pem",
        "storeFile": "build/default/intermediates/signing/debug/ohos_app_debug.p12",
        "keyAlias": "debugKey",
        "keyPassword": "123456",
        "storePassword": "123456",
        "profile": "build/default/intermediates/signing/debug/signed-profile.p7b",
        "signAlg": "SHA256withECDSA",
        "hashAlg": "SHA256"
      }
    }
  ]
} 